{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch IdentityHub.API",
            "env": {
                "reseed": "false"
            },
            "program": "${workspaceFolder}\\IdentityHub.API\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch IdentityHub.API and reseed",
            "env": {
                "reseed": "true"
            },
            "program": "${workspaceFolder}\\IdentityHub.API\\index.js"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Debug IdentityHub.UI",
            "cwd": "${workspaceFolder}/IdentityHub.UI",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceRoot}"
        }
    ]
}